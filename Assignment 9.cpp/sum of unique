//+
class Solution {
public:
    int sumOfUnique(vector<int>& nums) {
     //declear hash table
     unordered_map<int,int>map;
  for(int i=0;i<nums.size();i++){ 
    // for(auto x:nums)
    map[nums[i]++];
   }
int sum=0;
   //traverse map
        for(auto x:map){
            if(x.second==1)
            sum=sum+x.first;
        }
        return sum;
    }
};
