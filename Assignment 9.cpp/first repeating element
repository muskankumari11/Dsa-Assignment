class solution{
    public:
    int firstrepeated(int A[],int n)
    unordered_map<int,int>umap;
    for(int i=0;i<n;i++)
        umap[A[i]++];
        for(int i=0;i<n;i++)
        int key=A[i];
        auto temp=umap.find(key);
        int val=temp->second;
        if(val>1)
        return i+1;
        }
        return -1;
        }
        };
